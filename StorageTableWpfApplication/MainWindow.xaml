<Window x:Class="StorageTableWpfApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StorageTableWpfApplication"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
		<Button x:Name="button" Content="テーブル作成" HorizontalAlignment="Left" Height="30" Margin="10,18,0,0" VerticalAlignment="Top" Width="497" Click="button_Click"/>
		<Button x:Name="button1" Content="エンティティをテーブルに追加する" HorizontalAlignment="Left" Height="29" Margin="10,53,0,0" VerticalAlignment="Top" Width="497" Click="button1_Click"/>
		<Button x:Name="button2" Content="パーティション内のすべてのエンティティを取得する" HorizontalAlignment="Left" Height="30" Margin="10,87,0,0" VerticalAlignment="Top" Width="497" Click="button2_Click"/>
		<Button x:Name="button3" Content="エンティティのバッチを挿入する（100件）" HorizontalAlignment="Left" Height="31" Margin="10,122,0,0" VerticalAlignment="Top" Width="497" Click="button3_Click"/>

	</Grid>
</Window>
