<Window x:Class="StorageWpfApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StorageWpfApplication"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
		<Button x:Name="button" Content="Blob Containerの作成" HorizontalAlignment="Left" Height="28" Margin="30,19,0,0" VerticalAlignment="Top" Width="438" Click="button_Click"/>
		<Button x:Name="button1" Content="コンテナー内の BLOB を一覧表示する" HorizontalAlignment="Left" Height="25" Margin="30,52,0,0" VerticalAlignment="Top" Width="438" Click="button1_Click"/>
		<Button x:Name="button2" Content="追加 Blobへの書き込み" HorizontalAlignment="Left" Height="27" Margin="30,82,0,0" VerticalAlignment="Top" Width="438" Click="button2_Click"/>
		<Button x:Name="button3" Content="追加 Blobへの書き込み（20MBのムービー）" HorizontalAlignment="Left" Height="26" Margin="30,114,0,0" VerticalAlignment="Top" Width="438" Click="button3_Click"/>

	</Grid>
</Window>
